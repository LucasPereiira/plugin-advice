{
    "manifest_version": 3,
    "name": "Conselho do dia",
    "version": "1.0",
    "description": "Clique e receba um conselho para o seu dia.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "/assets/images/icon.png"
    },
    "permissions": [
      "tabs",
      "clipboardWrite"
    ]
  }
  